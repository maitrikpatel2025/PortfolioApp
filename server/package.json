  "scripts": {
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && node index.js",
    "server": "SET NODE_OPTIONS=--openssl-legacy-provider && nodemon index.js",
    "client": "SET NODE_OPTIONS=--openssl-legacy-provider && npm run start --prefix client",
